      "Reporting_Airline",
      "Airline_Name"
    ]
  },
  "highlight": {
    "fields": {
      "Airline_Name.semantic": {
        "type": "semantic",
        "number_of_fragments": 1,
        "fragment_size": 200
      }
    }
  }
}
